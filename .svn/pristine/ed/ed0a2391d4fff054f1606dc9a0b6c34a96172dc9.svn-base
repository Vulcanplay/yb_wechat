<ion-header>
  <ion-navbar>
    <ion-buttons start (click)="dismiss()">
      <button ion-button>
        <i class="iconfont lg icon-custom-arrowleft"></i>
      </button>
    </ion-buttons>
    <ion-title>城市</ion-title>
    <ion-searchbar *ngIf="false" placeholder="输入城市名或拼音查询" (ionInput)="filterItems($event)"></ion-searchbar>
  </ion-navbar>
</ion-header>
