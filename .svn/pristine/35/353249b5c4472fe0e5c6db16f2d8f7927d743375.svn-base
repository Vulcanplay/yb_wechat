page-mine-address{
  .item-ios h2{
    font-size: 1.5rem;
    padding:.5rem 0;
  }
  .item-ios p{
    font-size:1.3rem;
  }
  .radio-ios .radio-icon{
    width: 21px;
    border-radius: 5px;
    border:1px solid #e6e6e6;
  }
  .radio-ios-oranger .radio-checked{
    border:1px solid color($colors, oranger);
  }
  .radio-ios .radio-checked .radio-inner{
    left: 8px;
    top: 2px;
  }
  ion-icon.trash{font-size: 2.4rem; margin-right: 5px;}
  .content-ios, .content-md, .content-wp{background-color: #f3f3f3 !important;}
}
