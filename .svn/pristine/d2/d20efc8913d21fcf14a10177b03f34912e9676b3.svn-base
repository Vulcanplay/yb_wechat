.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shouji:before { content: "\e6de"; }

.icon-mima:before { content: "\e6a1"; }

.icon-yanzhengma:before { content: "\e66c"; }

html{
  width: 100%;
  height: 100%;
  font-size: 62.5%;
}
body{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  color: #ffffff;
  background: #000000 url(../images/bottom_1.png) no-repeat bottom .5em center;
  background-size: 98.66667px 22.66667px;
}
header{
  width: 100%;
  height: 16rem;
  background: url(../images/banner_1.jpg) no-repeat top center;
  background-size: contain;
}
.list{
  margin: 16px 32px;
}
.list > .item{
  padding-left: 16px;
  border-radius: 0;
  position: relative;
  font-size: 1.4rem;
  margin: 0;
  padding: 0;
  text-align: initial;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  overflow: hidden;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
  width: 100%;
  min-height: 4.4rem;
  font-weight: normal;
  line-height: normal;
  text-decoration: none;
}
.item i,.item input,.item button.send-code{
  -webkit-box-flex: initial;
  -webkit-flex: initial;
  -ms-flex: initial;
  flex: initial;
  color: #808080;
}
.item{
  border-bottom:1px solid #4e4e4e;
  margin-bottom: 8px !important;
}
.item i{
  font-size: 24px;
  margin: 8px;
}
.item input{
  border:0;
  outline: none;
  width: 100%;
  height: 30px;
  background-color: transparent;
  color:#ffffff;
  margin-top: 6px;
  font-size: 1.6rem;
}
.item button.send-code{
  background-color: transparent;
  border-color: transparent;
  padding: 6px 12px 6px 12px;
  white-space: nowrap;
  overflow: initial;
  border-left:1px solid #4d4d4d;
  border-radius-bottomleft: 10px;
  border-radius-topleft: 10px;
  outline: none;
}
.item button.send-code:active{
  color: #cfcfcf;
}
.submit{
  outline: none;
  border-color: transparent;
  background-color: #1a1a1a;
  display: block;
  width: 100%;
  color:#909090;
  padding: .6em 0;
  font-size: 1.7rem;
  margin-top: 32px;
}
.submit:active{
  background-color: #343434;
  color: #a0a0a0;
}
