  ion-item-divider{color:#838383 !important;}
  .white-bg{background-color: white;}
  .icon-img,.img-contain{background-position: center center;background-size: contain; background-repeat: no-repeat;}
  .img-contain{height: 4rem;margin: .2em;}
  .icon-img.icar-logo{min-width: 40px;}
  .item-ios{font-size: 1.5rem;}
  .brandbox-shortcuts{
    position: absolute;
    z-index: 9999;
    top:15%;
    right:0;
    bottom:10%;
    width: 30px;
    font-size: 13px;
    display: -webkit-box;
    display: box;
    -webkit-box-direction: normal;
    -webkit-box-orient:vertical;
    -webkit-flex-direction:inherit;
    -moz-flex-direction: vertical;
    -ms-flex-direction: inherit;
    flex-direction: inherit;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    -moz-justify-content: center;
    justify-content: center;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  .brandbox-shortcuts a{
    text-align: center;
    color:#8a8a8a;
    -webkit-box-flex: 1;
    text-decoration: none;
    box-flex: 1;
    display:-webkit-box;
    display:-webkit-flex;
    display:flex;
    -webkit-box-pack:center;
    -ms-justify-content:center;
    -webkit-justify-content:center;
    justify-content:center;
    -webkit-box-align:center;
    -ms-align-items:center;
    -webkit-align-items:center;
    align-items:center;
  }
