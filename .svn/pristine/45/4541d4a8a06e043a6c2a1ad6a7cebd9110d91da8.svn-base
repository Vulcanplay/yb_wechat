page-ewp-order{
  .vehicle{width: 98%; margin: 0 auto;}
  .vehicle .icon-img{background-position: center center;background-size: 100%; background-repeat: no-repeat;margin: 0!important;margin-right: 12px !important;}
  .vehicle .icon-img.icar-logo{min-width: 75px; min-height: 75px;}
  .vehicle h2{margin-bottom: .5em; font-size: 1.6rem; font-weight: bold;}
  .vehicle p{margin-top: .5em;}

  .date-km{text-align: center; border-bottom: .8rem solid #f5f5f5;}
  .date-km ion-icon{font-size: 4.5rem; margin: .3em 0 .5em 0;}
  .date-km h3{ margin: 0; font-size: 1.7rem;}

  .list-block .title-card{ margin-top: .8em; margin-bottom: .4em; font-size: 1.88rem; margin-left: 16px; color:#666666; font-weight: 700;}
  .list-block.list-ios > .item-block:last-child, .list-block.list-ios > .item-wrapper:last-child .item-block{border-bottom: .8rem solid #f5f5f5;}
  .list-block.info.list-ios .item-block .item-inner{border: none;}
  .list-block .call h4{ font-size: 1.6rem; margin: .8em 0;}

  .item.item-ios .checkbox-ios{margin-right:.7em;}
  .information{position: absolute;font-size: 2rem !important;top: 30%;left: auto;}
  ion-label {white-space: normal;}
}
